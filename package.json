{
  "name": "agent",
  "version": "1.0.0",
  "main": "dist/main.js",
  "scripts": {
    "test": "jest",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:all": "jest --selectProjects unit integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:production-readiness": "jest --selectProjects integration --testNamePattern='Production Readiness'",
    "test:discovery-validation": "jest --selectProjects integration --testNamePattern='Discovery Pipeline Validation'",
    "dev": "ts-node src/main.ts",
    "build": "tsc",
    "extract": "ts-node src/main.ts",
    "extract-enhanced": "ts-node src/main.ts --enhanced",
    "discover-shows": "ts-node src/cli/discoverShows.ts",
    "compare-extraction": "ts-node src/compare-baseline.ts",
    "query-baseline": "ts-node src/query-baseline.ts",
    "query-production": "ts-node src/query-production.ts",
    "query-production-api": "ts-node src/query-production-api.ts",
    "query-local-unenriched": "ts-node src/query-local-unenriched.ts",
    "enrich-local-shows": "ts-node src/cli/enrichLocalShows.ts",
    "compare-local-to-production": "ts-node src/analysis/compareLocalToProduction.ts",
    "batch-extraction": "ts-node src/cli/batchExtraction.ts",
    "compare-batch-results": "ts-node src/analysis/batchComparison.ts",
    "query-unenriched-shows": "ts-node src/query-unenriched-shows.ts",
    "query-production-enriched": "ts-node src/query-production-enriched.ts",
    "enrich-test-shows": "ts-node src/cli/enrichTestShows.ts",
    "compare-ai-vs-production": "ts-node src/analysis/compareAIvsProduction.ts",
    "test-dates": "npm run extract-enhanced https://www.pacegallery.com/exhibitions/alicja-kwade-telos-tales/ && npm run compare-extraction",
    "test-press-release": "npm run extract-enhanced https://www.pacegallery.com/exhibitions/alicja-kwade-telos-tales/ && npm run compare-extraction"
  },
  "keywords": [
    "art",
    "extractor",
    "ai",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "description": "AI Show Extractor - Automated art show data extraction system",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^22.10.2",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.7",
    "openai": "^4.73.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5"
  }
}
